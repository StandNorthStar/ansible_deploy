{
    "debug": true,
    "hostname": "",
    "ip": "",
    "plugin": {
        "enabled": false,
        "dir": "./plugins",
        "git": "https://github.com/open-falcon/plugin.git",
        "logs": "./logs"
    },
    "plugin_sr": {
        "enabled": {{ agent.plugin_enable }},
        "plugins": [
	    	    "hw",
	    	    "vm",
	    	    "ceph",
	    	    "rabbitmq",
	    	    "haproxy",
	    	    "memcached",
                "mysql",
                "services_all",
                "services_controller",
                "services_compute_net",
                "services_controller_01"
		],
        "logs": "./logs"
    },
    "heartbeat": {
        "enabled": true,
        "addr": "{{ agent.heartbeat }}:{{ agent.heartbeatport }}",
        "interval": 60,
        "timeout": 1000
    },
    "transfer": {
        "enabled": true,
        "addrs": [
            "{{ agent.transfer }}:{{ agent.transfer_port }}"
        ],
        "interval": 60,
        "timeout": 1000
    },
    "http": {
        "enabled": {{ agent.http_enable }},
        "listen": ":{{ agent.http_listen }}",
        "backdoor": false
    },
    "collector": {
        "ifacePrefix": ["eth", "em"]
    },
    "ignore": {
        "df.bytes.free": true,
        "df.bytes.total": true,
        "df.bytes.used": true,
        "df.bytes.used.percent": true,
        "df.inodes.total": true,
        "df.inodes.free": true,
        "df.inodes.used": true,
        "df.inodes.used.percent": true,
        "mem.memtotal": true,
        "mem.memused": true,
        "mem.memused.percent": true,
        "mem.memfree": true,
        "mem.swaptotal": true,
        "mem.swapused": true,
        "mem.swapfree": true
    }
}
