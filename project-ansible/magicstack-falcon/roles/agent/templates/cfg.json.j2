{
    "debug": true,
    "hostname": "",
    "ip": "",
    "plugin": {
        "enabled": false,
        "dir": "./plugin",
        "git": "https://github.com/open-falcon/plugin.git",
        "logs": "./logs"
    },
    "heartbeat": {
        "enabled": true,
        "addr": "{{ agent.heartbeat_addr }}:{{ agent.heartbeat_addr_port }}",
        "interval": 60,
        "timeout": 1000
    },
    "transfer": {
        "enabled": true,
        "addrs": [
            "{{ agent.transfer }}:{{ agent.transfer_port }}"
        ],
        "interval": {{ agent.interval_time }},
        "timeout": 1000
    },
    "http": {
        "enabled": true,
        "listen": ":{{ agent.http_listen }}",
        "backdoor": false
    },
    "collector": {
        "ifacePrefix": [ {% for item in agent['collector_net'] %}"{{ item }}"{% if not loop.last %},{% endif %}{% endfor %}]
    },
    "ignore": {
        "cpu.busy": true,
        "df.bytes.free": true,
        "df.bytes.total": true,
        "df.bytes.used": true,
        "df.bytes.used.percent": true,
        "df.inodes.total": true,
        "df.inodes.free": true,
        "df.inodes.used": true,
        "df.inodes.used.percent": true,
        "mem.memtotal": true,
        "mem.memused": true,
        "mem.memused.percent": true,
        "mem.memfree": true,
        "mem.swaptotal": true,
        "mem.swapused": true,
        "mem.swapfree": true
    }
}
