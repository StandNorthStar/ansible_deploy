[Unit]
Description=snmp agent
After=network.target mysql.service
After=syslog.target

[Install]
WantedBy=multi-user.target
Alias=snmp agent.service

[Service]
User=root
Group=root
Type=forking
# Start main service
ExecStart={{ INSTALL_PATH }}/snmp-collector/control start 
ExecStop={{ INSTALL_PATH }}/snmp-collector/control stop
Restart=always