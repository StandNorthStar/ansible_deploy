{
    "debug": true,
    "minStep": 30,
    "http": {
        "enabled": true,
        "listen": "{{ transfer.listen }}:{{ transfer.port }}"
    },
    "rpc": {
        "enabled": true,
        "listen": "0.0.0.0:8433"
    },
    "socket": {
        "enabled": true,
        "listen": "{{ transfer.socket_ip }}:{{ transfer.socket_port }}",
        "timeout": 3600
    },
    "judge": {
        "enabled": true,
        "batch": 200,
        "connTimeout": 1000,
        "callTimeout": 5000,
        "maxConns": 32,
        "maxIdle": 32,
        "replicas": 500,
        "cluster": {
            "judge-00" : "{{ transfer.judge_ip }}:{{ transfer.judge_port }}"
        }
    },
    "graph": {
        "enabled": true,
        "batch": 200,
        "connTimeout": 1000,
        "callTimeout": 5000,
        "maxConns": 32,
        "maxIdle": 32,
        "replicas": 500,
        "cluster": {
            "graph-00" : "{{ graph.migrate_graph }}:{{ graph.migrate_graph_port }}"
        }
    },
    "tsdb": {
        "enabled": false,
        "batch": 200,
        "connTimeout": 1000,
        "callTimeout": 5000,
        "maxConns": 32,
        "maxIdle": 32,
        "retry": 3,
        "address": "{{ transfer.tsdb_ip }}:{{ transfer.tsdb_port }}"
    },
    "kafka": {
        "enabled": true,
        "batch": 200,
        "connTimeout": 1000,
        "callTimeout": 5000,
        "maxConns": 32,
        "maxIdle": 32,
        "retry": 3,
        "cluster": {
                "kafka": "127.0.0.1:9092"
        },
        "topic": "openfalcon",
	"support": ["cpu.load.1min","cpu.load.5min"]
    }

}
