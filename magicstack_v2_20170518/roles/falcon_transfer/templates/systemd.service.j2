[Unit]
Description=Open-falcon transfer
After=network.target mysql.service
After=syslog.target

[Install]
WantedBy=multi-user.target
Alias=openfalcon-transfer.service

[Service]
User=root
Group=root
Type=forking
# Start main service
ExecStart={{ INSTALL_PATH }}/transfer/control start 
ExecStop={{ INSTALL_PATH }}/transfer/control stop
Restart=always
